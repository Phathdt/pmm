{
  "name": "@optimex-pmm/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve api-server",
    "build": "yarn typecheck && nx build api-server",
    "format": "prettier --write \"**/*.ts\"",
    "typecheck": "tsc --project tsconfig.base.json --noEmit",
    "lint": "yarn typecheck && eslint \"apps/**/*.ts\" \"libs/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "ctix": "node generate-indexes.js",
    "lint-staged": "lint-staged",
    "commit": "yarn ctix && git-cz",
    "prepare": "husky",
    "check:unused": "node unused-detector.js",
    "check:unused:ci": "node unused-detector.js --exit-code"
  },
  "resolutions": {
    "@bull-board/express/express": "5.0.1"
  },
  "private": true,
  "dependencies": {
    "@bull-board/api": "^6.9.6",
    "@bull-board/express": "^6.9.6",
    "@bull-board/nestjs": "^6.9.6",
    "@coral-xyz/anchor": "^0.31.0",
    "@keyv/redis": "^4.4.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@optimex-xyz/market-maker-sdk": "^0.9.0-dev-b026721",
    "@prisma/client": "^6.7.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "1.98.0",
    "axios": "^1.9.0",
    "bitcoinjs-lib": "^7.0.0-rc.0",
    "bs58": "^6.0.0",
    "bull": "^4.16.5",
    "cache-manager": "^6.4.3",
    "ecpair": "^3.0.0",
    "ethers": "^6.13.7",
    "ethers-decode-error": "^2.1.3",
    "ioredis": "^5.7.0",
    "nestjs-pino": "^4.4.0",
    "nestjs-prisma": "^0.25.0",
    "nestjs-zod": "^5.0.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "redis": "^5.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "tiny-secp256k1": "^2.2.3",
    "uuid": "^11.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.26.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.0",
    "@nx/eslint": "21.0.3",
    "@nx/eslint-plugin": "21.0.3",
    "@nx/jest": "21.0.3",
    "@nx/js": "21.0.3",
    "@nx/nest": "21.0.3",
    "@nx/node": "21.0.3",
    "@nx/web": "21.0.3",
    "@nx/webpack": "21.0.3",
    "@nx/workspace": "21.0.3",
    "@swc-node/register": "~1.10.10",
    "@swc/core": "~1.11.24",
    "@swc/helpers": "~0.5.17",
    "@types/jest": "^29.5.14",
    "@types/node": "~22.15.17",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-unused-imports": "^4.2.0",
    "git-cz": "^4.9.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
